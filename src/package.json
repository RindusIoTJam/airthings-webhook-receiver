{
  "name": "airthings-webhook",
  "version": "0.1.0",
  "description": "webhook endpoint for Airthings to post to.",
  "scripts": {
    "start": "node ./server.js",
    "development": "nodemon ./server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RindusIoTJam/airthings-webhook-receiver"
  },
  "keywords": [
    "Node.js",
    "Javascript",
    "Express"
  ],
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  },
  "nodemonConfig": {
    "verbose": false,
    "ignore": [
      "views/**",
      ".git",
      "node_modules/**"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js,json",
    "delay": 2000
  }
}
